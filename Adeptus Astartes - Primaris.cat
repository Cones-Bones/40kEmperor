    <selectionEntry id="b544-e1e3-f753-93a5" name="Primaris " hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="94be-346a-aa7d-ca97" type="max"/>
