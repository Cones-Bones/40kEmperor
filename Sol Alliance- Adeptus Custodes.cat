              <modifiers>
                <modifier type="increment" field="e7de-8dcf-403a-b76a" value="15">
                  <repeats>
                    <repeat field="selections" scope="07fc-af15-a40f-58be" value="1" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="model" repeats="1" roundUp="false"/>
                  </repeats>
